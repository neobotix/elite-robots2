<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="cs66">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import cs66 urdf file -->
    <xacro:include filename="$(find elite_description)/urdf/cs66_description.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="cs66.ros2_control.xacro" />


    <xacro:cs66_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
